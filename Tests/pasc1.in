PROGRAM euclid (input, output);
var x,a,b : integer;
var c : real;

FUNCTION gcd_recursive(u, v : integer) : integer;
BEGIN
  IF u mod v <> 0 THEN
    gcd_recursive := gcd_recursive(v, u mod v)
  ELSE
    gcd_recursive := v
END;
{ main program starts here }
BEGIN
  x := gcd_recursive(a,c)
END.
